<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.rank">
	<select id="avgsalary"  resultType="java.util.HashMap">
		select * from SALARY order by AVG_SALARY desc  
	</select>
	
	<select id="salary"  resultType="java.util.HashMap">
		select * from SALARY order by AVG_SALARY desc  
	</select>
	
	<select id="local"  parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from RANK where WKP_ADRS like #{local} order by SALARY desc  
	</select>
</mapper>